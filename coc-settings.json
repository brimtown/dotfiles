{
  "tsserver.log": "normal",
  "colors.enable": true,
  "diagnostic.errorSign": "┃",
  "diagnostic.warningSign": "┃",
  "diagnostic.infoSign": "┃",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "eslint.experimental.useFlatConfig": true,
  "typescript.updateImportsOnFileMove.enable": "always",
  "javascript.updateImportsOnFileMove.enable": "always",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "tsserver.maxTsServerMemory": 24576,
  "eslint.filetypes": [
      "javascript",
      "javascript.jsx",
      "javascriptreact",
      "typescript",
      "typescript.tsx",
      "typescriptreact"
  ],
  "eslint.nodePath": ".yarn/sdks",
  "eslint.packageManager": "yarn",
  "prettier.prettierPath": ".yarn/sdks/prettier",
  "tsserver.tsdk": "~/.config/coc/extensions/node_modules/coc-tsserver/node_modules/typescript/lib",
  "workspace.workspaceFolderCheckCwd": false,
  "inlayHint.enable": false,
  "files.watcherExclude": {
        "bazel-*/": true,
  },
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    },
    // "go": {
    //   "command": "gopls",
